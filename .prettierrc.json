{
  "$schema": "https://json.schemastore.org/prettierrc",
  "plugins": ["@prettier/plugin-oxc"],
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "experimentalTernaries": true,
  "printWidth": 80,
  "useTabs": false,
  "bracketSpacing": true,
  "objectWrap": "collapse",
  "bracketSameLine": true,
  "endOfLine": "lf",
  "trailingComma": "es5",
  "arrowParens": "avoid",
  "quoteProps": "as-needed"
}
